def sendEmail(to, content):

    server = smtplib.SMTP('smtp.gmail.com', 587)

    server.ehlo()

    server.starttls()

    server.login('******@gmail.com', 'ENTER_YOUR_PASSWORD')

    server.sendmail('********@gmail.com', to, content)

    server.close()

def speak(audio):

    engine.say(audio)

    engine.runAndWait()

