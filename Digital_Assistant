            import os

            import shutil

            path = r'C:\Users\Talha\Downloads\Talha 1'

            img = r'C:\Users\Talha\Desktop\Images'

            txt = r'C:\Users\Talha\Desktop\texts'

            pdf = r'C:\Users\Talha\Desktop\pdf'

